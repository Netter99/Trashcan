<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app>

  <servlet>
    <servlet-name>AddAccountInfoServlet</servlet-name>
    <servlet-class>org.servlet.AddAccountInfoServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>AddAccountInfoServlet</servlet-name>
    <url-pattern>/wxaddaccountinfo</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UpLoadPicServlet</servlet-name>
    <servlet-class>org.servlet.UpLoadPicServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>UpLoadPicServlet</servlet-name>
    <url-pattern>/wxuploadpic</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>WebLoginServlet</servlet-name>
    <servlet-class>org.servlet.WebLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>WebLoginServlet</servlet-name>
    <url-pattern>/weblogin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ChangeAccountPwdServlet</servlet-name>
    <servlet-class>org.servlet.ChangeAccountPwdServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ChangeAccountPwdServlet</servlet-name>
    <url-pattern>/wxchangeaccountpwd</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>WxLoginServlet</servlet-name>
    <servlet-class>org.servlet.WxLoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>WxLoginServlet</servlet-name>
    <url-pattern>/wxlogin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>ChangeGoodsServlet</servlet-name>
    <servlet-class>org.servlet.ChangeGoodsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>ChangeGoodsServlet</servlet-name>
    <url-pattern>/wxchangegoods</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>LaunchTimeServlet</servlet-name>
    <servlet-class>org.servlet.LaunchTimeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>LaunchTimeServlet</servlet-name>
    <url-pattern>/wxlaunchtime</url-pattern>
  </servlet-mapping>

